

{% block title %}List of roles{% endblock %}

{% block body %}
    <h1>List of roles</h1>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Nom</th>
            </tr>
        </thead>
        <tbody>
            {% for role in roles %}
                <tr>
                    <td>{{ role.id }}</td>
                    <td>{{ role.nom }}</td>
                   
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}
